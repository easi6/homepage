body
  font-family: 'Open sans', sans-serif
  font-size: 14px
  //background:url(/images/extra_clean_paper_@2X.png)
  color : #33333
  padding:0
  margin:0

::selection
  background: #ffbe71; /* Safari */

::-moz-selection
  background: #ffbe71; /* Firefox */

#nav.background_green
  background-color: #80AF82

#nav.background_red
  background-color: #E44324

#nav.background_orange
  background-color: #FF9700

#nav.background_gray
  background-color: #6E6E6E

#nav
  z-index: 999
  position:fixed
  border-radius: 65px
  width:130px
  height:130px
  top:50%
  left: 70%
  h1
    margin-top:30px
    a.logo
      display:block
      background: url(/images/logo2.png) no-repeat
      height: 54px
      width: 100px
      text-indent:-99999px
      margin:auto
  nav
    display:none

#nav:hover
  z-index: 999
  position:fixed
  border-radius: 100px
  width:200px
  height:200px
  a.logo
    display:block
    background: url(/images/logo2.png) no-repeat
    height: 54px
    width: 100px 
    text-indent:-99999px
    margin-top: 10px
    margin:auto
  nav
    margin-top:40px
    margin-left:2px
    display:block
    ol
      width: 200px
      li.none
        display: none
      li.left
        margin-right:15px
        a
          text-align: right
      li.right
        a
          text-align:left
      li
        margin-bottom: 20px
        width: 90px
        float:left
        a
          width: 90px
        a.nav-item
          float:left
          font-size: 15px
          color: rgba(0,0,0, 0.4)
          text-decoration: none
        a.nav-item:visited
          font-size: 15px
          color: rgba(0,0,0, 0.4)
          text-decoration: none
        a.nav-item:hover
          font-size: 15px
          color: rgba(255,255,255, 0.4)
          text-decoration: none
      li.active
        a.nav-item
          color: rgba(255,255,255, 1.0)

#main-container
  padding: 0
  margin-top:0
  background:white
  //box-shadow: 0 0 10px #888
  margin-bottom:30px
  //border-radius:10px
  width:950px
  margin:auto

#about_header_carousel
  display: block
  div.carousel-inner
    height: 410px
    p.header_title
      padding-top: 110px
  ol
    right: 0
    position: relative
    margin: 0
    padding: 0
    height: 60px
    li.active
      background-color: #ff0000
    li
      background-color: #eee

.text-600
  font-family: 'Open sans', sans-serif
  font-weight: 600

.text-400-italic
  font-style: italic
  font-weight: 400

.header_title
  font-family: 'Open sans', sans-serif
  font-weight: 600
  font-size: 65px
  line-height: 117%

section
  padding-top:100px

#intro
  p
    height:90px
    margin-bottom:10px
    line-height: 171%
  .about_desc_left
    float:left
    width:430px
  .about_desc_right
    width: 510px
    margin-left: 440px

#projects
  .project_img
    width: 470px
    height: 470px
    margin-bottom: 15px
    background-color: #eee
  #project_dnd
    float:left
    width: 470px;
  #project_lb
    width:470px
    margin-left: 480px
  p
    line-height: 171%

#members
  .members
    width:950px
    .member
      float:left
      display:block
      width:230px
      margin-right: 10px
      margin-bottom: 40px
      img
        width:230px
        height:230px
        margin-bottom: 10px
      p
        margin-bottom: 10px
      .member-name
        font-size: 16px
        font-weight: 700
        line-height: 140%
      .member-title .email
        font-size: 12px
        font-weight: 600
        line-height: 158%
    .no-margin-right
      margin-right: 0px

#contact
  margin-bottom: 500px
  #map_canvas
    width:950px
    height:410px
    display:block
    margin-bottom:30px
  .contact
    a
      color: #333333
      font-size: 14px
      line-height: 171%


a.email 
  text-decoration:none
  font-style:normal
  color:#59F
.address
  font-size:15pt
  color:#333
  text-decoration:none
  &:hover
    text-decoration:underline
  sup
    vertical-align:super
    font-size:small
